offset_left = -424.0
offset_top = 1191.0
offset_right = 370.0
offset_bottom = 1293.0
text = "Pengguna harus bisa memahami konten dan navigasi tanpa bingung. Gunakan heading secara hierarkis: H1 untuk judul utama, H2 untuk subjudul, H3 untuk detail dalam subbagian."
intended_structure = 2
