offset_left = -422.0
offset_top = 747.0
offset_right = 359.0
offset_bottom = 849.0
text = "Fokus yang terlihat saat menggunakan tab sangat penting, begitu juga penggunaan shortcut logis dan urutan tab yang sesuai. Hindari elemen yang terjebak atau tidak bisa diakses sama sekali."
intended_structure = 2
part = 2

[node name="doc_text9" parent="texts" instance=ExtResource("2_5w8pr")]
