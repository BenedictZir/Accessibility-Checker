offset_left = -422.0
offset_top = 1355.0
offset_right = 359.0
offset_bottom = 1457.0
text = "Gunakan bahasa yang jelas, hindari jargon teknis berlebihan, dan berikan feedback saat pengguna melakukan aksi (seperti mengirim form atau mengklik tombol)."
intended_structure = 2
part = 3

